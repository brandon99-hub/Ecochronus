{
  "name": "mission-game-backend",
  "version": "1.0.0",
  "description": "Backend API for mission-based mobile game",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run build:frontend && tsc",
    "build:frontend": "npm --prefix ../frontend install && npm --prefix ../frontend run build",
    "start": "node dist/server.js",
    "db:generate": "tsx src/database/generate.ts",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts"
  },
  "keywords": ["backend", "api", "game", "missions"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "@google-cloud/storage": "^7.7.0",
    "zod": "^3.22.4",
    "google-auth-library": "^9.4.1",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "drizzle-kit": "^0.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

